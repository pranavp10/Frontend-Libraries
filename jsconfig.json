{
  "compilerOptions": {
    "baseUrl": ".",
    "paths": {
      "@/components/*": [
        "src/components/*"
      ],
      "@/styles/*": [
        "src/styles/*"
      ],
      "@/lib/*": [
        "src/lib/*"
      ],
      "@/data/*": [
        "src/data/*"
      ],
      "@/globalState/*": [
        "src/globalState/*"
      ],
      "@/config/*": [
        "src/config/*"
      ],
      "@/hooks/*": [
        "src/hooks/*"
      ],
      "@/api/*": [
        "src/api/*"
      ],
    }
  }
}
